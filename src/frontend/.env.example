# Azure Blob Storage Configuration
# Copy this file to .env and fill in your values

# Option 1: Connection String (for development)
VITE_AZURE_STORAGE_CONNECTION_STRING=your_connection_string_here

# Option 2: Account Name + SAS Token (recommended for production)
VITE_AZURE_STORAGE_ACCOUNT_NAME=your_account_name
VITE_AZURE_STORAGE_SAS_TOKEN=your_sas_token

# Container Name (required)
VITE_AZURE_STORAGE_CONTAINER_NAME=your_container_name
