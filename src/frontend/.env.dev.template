# Development environment variables template
# Copy this file to .env.dev and fill in your local values
# .env.dev is gitignored so your secrets stays local

# Environment
VITE_ENVIRONMENT=development

# Logging
VITE_LOG_LEVEL=debug

# Telemetry (noop - no collector running)
VITE_TELEMETRY_ENABLED=false
VITE_OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318/v1/traces
VITE_SERVICE_VERSION=0.1.0

# API
VITE_API_BASE_URL=http://localhost:8000

# Add any local secrets below
# VITE_API_KEY=
