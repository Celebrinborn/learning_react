# Development environment variables template
# Copy this file to .env.dev and fill in your local values
# .env.dev is gitignored so your secrets stay local

# Environment
ENVIRONMENT=development

# Logging
LOG_LEVEL=DEBUG
LOG_FORMAT=text

# Telemetry (noop - no collector running)
TELEMETRY_ENABLED=false
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
SERVICE_VERSION=0.1.0

# Server
PORT=8000
CORS_ORIGINS=http://localhost:5173

# Add any local secrets below
# DATABASE_URL=
# API_KEY=
